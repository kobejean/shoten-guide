{
  "name": "shouten-guide",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "run-p watch server",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "server": "nodemon server.js --watch server.js --watch public/App.js",
    "update-submodules": "git submodule update --init --recursive --remote"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "dynamic-import-polyfill": "^0.1.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "sirv-cli": "^1.0.0",
    "svelte-i18n": "^3.1.0",
    "svelte-routing": "file:submodules/svelte-routing"
  }
}

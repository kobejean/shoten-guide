{
  "name": "shoten-guide",
  "description": "shoten-guide",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "SAPPER_TIMESTAMP=$(date +%s) sapper build --legacy",
    "export": "sapper export --legacy",
    "publish": "vercel -e SAPPER_TIMESTAMP=$(date +%s)",
    "publish:docs": "jsdoc -r ./src -c jsdoc.config.js --readme README.md",
    "start": "node __sapper__/build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test": "run-p --race dev cypress:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "polka": "next",
    "sirv": "^1.0.11",
    "svelte-i18n": "^3.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-url": "^6.0.0",
    "@rollup/pluginutils": "^4.1.0",
    "braintree-jsdoc-template": "^3.3.0",
    "cypress": "^6.6.0",
    "jsdoc": "^3.6.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.40.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.29.1",
    "sass": "^1.32.8",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.6.9"
  }

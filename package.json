{
  "name": "shoten-guide",
  "description": "shoten-guide",
  "version": "0.0.1",
  "scripts": {
    "rename-hack": "cp -R src/node_modules_hack/. src/node_modules",
    "dev": "npm run rename-hack && sapper dev",
    "build": "npm run rename-hack && SAPPER_TIMESTAMP=$(date +%s) sapper build --legacy",
    "export": "npm run rename-hack && sapper export --legacy",
    "publish": "vercel -e SAPPER_TIMESTAMP=$(date +%s)",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-i18n": "^3.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "@rollup/pluginutils": "^4.1.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.33.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "sass": "^1.29.0",
    "svelte": "^3.29.7",
    "svelte-preprocess": "^4.6.0"
  }
}
